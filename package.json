{
  "name": "spotify-liked-song-genres",
  "version": "2.1.0",
  "description": "Spotify application sorting liked songs into playlists by genre",
  "main": "src/App.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nodemon src/App.ts debug",
    "start": "ts-node src/App.ts",
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc -p .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bemoty/spotify-liked-song-genres.git"
  },
  "keywords": [
    "spotify",
    "spotify-application",
    "music"
  ],
  "author": "Joshua Winkler <josh@bemoty.dev>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bemoty/spotify-liked-song-genres/issues"
  },
  "homepage": "https://github.com/bemoty/spotify-liked-song-genres#readme",
  "dependencies": {
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "node-cron": "^3.0.2",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.2",
    "@types/node-cron": "^3.0.8",
    "@types/spotify-web-api-node": "^5.0.7",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.5"
  }
}
